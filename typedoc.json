{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "tsconfig": "./tsconfig.build.json",
  "excludeInternal": false,
  "excludePrivate": false,
  "useTsLinkResolution": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  }
}
